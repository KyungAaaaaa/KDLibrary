<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" styleClass="vBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label styleClass="textLabel" text="날짜">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <Label id="lbDate" styleClass="textLabel" text="Label" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextArea id="txaContent" prefHeight="200.0" prefWidth="200.0" styleClass="textField" />
         </children>
      </HBox>
      <Button id="btnAdd" mnemonicParsing="false" text="추가" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
